{
  "name": "react-form-master",
  "version": "0.0.1",
  "description": "React Form Master",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/react-form-master.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webbestmaster/react-form-master/issues"
  },
  "homepage": "https://github.com/webbestmaster/react-form-master#readme",
  "scripts": {
    "start": "cross-env BRANCH_NAME=`git symbolic-ref --short HEAD` webpack-dev-server --progress --no-inline",
    "start:hot-css": "cross-env BRANCH_NAME=`git symbolic-ref --short HEAD` webpack-dev-server --progress --hot",
    "start-dev-server:build": "cross-env BRANCH_NAME=`git symbolic-ref --short HEAD` NODE_ENV=production IS_BUILD_SERVER=YES webpack-dev-server --mode=production --progress",
    "build": "cross-env BRANCH_NAME=`git symbolic-ref --short HEAD` NODE_ENV=production webpack --mode=production --progress",
    "webpack": "webpack --mode=development --progress",
    "check": "npm run eslint; npm run stylelint; npm run flow;",
    "check:fix": "npm run prettier; npm run eslint:fix; npm run stylelint:fix; npm run flow;",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**/*.scss",
    "stylelint:fix": "stylelint ./**/*.scss --fix",
    "flow": "flow stop; flow;",
    "prettier": "prettier --write \"./**/*.js\"",
    "mdlint": "eslint --ext md .",
    "mdlint:fix": "eslint --ext md . --fix",
    "flow-coverage": "flow-coverage-report --config ./flow-coverage-config.json",
    "zip": "zip -9 --exclude=*node_modules* -r my-project_`git symbolic-ref --short HEAD`-`date +%Y-%m-%d-%H-%M-%S` ./",
    "pre-commit-hook": "npm run eslint && npm run stylelint && npm run flow",
    "postinstall": "cp ./dev/git/hooks/* ./.git/hooks/;",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.2",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-file-loader": "^1.1.1",
    "babel-plugin-flow-runtime": "^0.19.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "base64-inline-loader": "^1.1.1",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "css-modules-flow-types-loader": "^1.3.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-unicorn": "^12.0.1",
    "flow-bin": "^0.108.0",
    "flow-coverage-report": "^0.6.1",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.6.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.14",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "terser-webpack-plugin": "^2.1.0",
    "unused-files-webpack-plugin": "^3.4.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
